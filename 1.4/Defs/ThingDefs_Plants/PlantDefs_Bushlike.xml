<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<ThingDef ParentName="ToxForestBushBase">
		<defName>Outland_PlantToxania</defName>
		<label>toxania</label>
		<description>A bush native to the Toxic Forest</description>
		<graphicData>
			<texPath>Outland/ToxicForest/Plants/ToxaniaBush</texPath>
			<drawSize>1.15</drawSize>
			<shadowData>
				<volume>(0.55, 0.2, 0.2)</volume>
			</shadowData>
		</graphicData>
		<uiIconScale>0.85</uiIconScale>
		<plant>
			<growDays>3</growDays>
			<pollution>CleanOnly</pollution>
		</plant>
	</ThingDef>

