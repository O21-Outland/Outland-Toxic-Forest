<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<ThingDef ParentName="ToxForestPlantBase">
		<defName>Outland_PlantToxstalks</defName>
		<label>toxstalks</label>
		<description>Short stalks that resemble a dense grass.</description>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
			<Flammability>1.3</Flammability>
			<Nutrition>0.50</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Outland/ToxicForest/Plants/Toxstalk_Clean</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<hideAtSnowDepth>0.5</hideAtSnowDepth>
		<ingestible />
		<plant>
			<growDays>2.5</growDays>
			<pollutedGraphicPath>Outland/ToxicForest/Plants/Toxstalk_Pollut</pollutedGraphicPath>
			<harvestWork>40</harvestWork>
			<maxMeshCount>9</maxMeshCount>
			<visualSizeRange>0.4~0.6</visualSizeRange>
			<topWindExposure>0.4</topWindExposure>
			<wildOrder>1</wildOrder>
		</plant>
	</ThingDef>

